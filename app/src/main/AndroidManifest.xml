<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mstc.moyu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScheduleFragment"
            android:label="@string/title_schedule_fragment_activity" />
        <activity
            android:name=".GroupFragment"
            android:label="@string/title_group_fragment_activity" />
        <activity
            android:name=".AboutmeFragment"
            android:label="@string/title_aboutme_fragment_activity" />
        <activity android:name=".AddItemActivity" />

        <service
            android:name="com.mstc.db.DataBaseService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>
